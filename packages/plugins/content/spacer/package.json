{
  "name": "@react-page/plugins-spacer",
  "version": "0.0.3",
  "main": "./lib/index.js",
  "module": "./lib-es/index.js",
  "typings": "./lib/index.d.ts",
  "author": "ORY GmbH",
  "license": "GNU Lesser General Public License v3.0",
  "scripts": {
    "build": "npm-run-all --parallel build:lib build:css",
    "build:watch": "npm-run-all --parallel build:lib:watch build:css:watch",
    "build:lib": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-es.json",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:css": "postcss -c ../../../../postcss.config.js --dir lib/ src/index.css",
    "build:css:watch": "npm-run-all build:css -- -w",
    "docs": "documentation build ./lib/**/*.js --format md --github -o ../../../../docs/api/plugins/spacer.md",
    "prebuild": "npm-run-all clean",
    "clean": "rimraf \"lib\" && rimraf \"lib-es\""
  },
  "peerDependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@material-ui/styles": "*",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@react-page/core": "^0.0.3",
    "@react-page/ui": "^0.0.3",
    "classnames": "2.2.6",
    "react-resizable": "^1.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "2ff40c835eb89ae530b3ed159b89e627e2e14084"
}
